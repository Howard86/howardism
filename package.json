{
  "name": "howardism",
  "version": "1.0.21",
  "private": true,
  "repository": "https://github.com/howard86/howardism",
  "scripts": {
    "build": "lerna run build",
    "lint": "eslint . --ext ts --ext tsx",
    "format": "prettier --write \"**/*.(js|ts|tsx)\"",
    "type-check": "lerna run type-check --stream",
    "test": "lerna run test --stream",
    "update": "lerna version --no-push --include-merged-tags && pnpm version --patch && pnpm changelog",
    "changelog": "gitmoji-changelog && git add CHANGELOG.md && git commit -m \":memo: docs: update CHANGELOG\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@emotion/babel-plugin": "^11.10.2",
    "@next/bundle-analyzer": "^12.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.19",
    "@types/react": "^18.0.21",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "commitlint": "^17.1.2",
    "commitlint-config-gitmoji": "^2.2.5",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-testing-library": "^5.7.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "jest-matchmedia-mock": "^1.1.0",
    "jest-watch-typeahead": "^2.2.0",
    "lerna": "^5.5.2",
    "lint-staged": "^13.0.3",
    "next": "^12.3.1",
    "next-router-mock": "^0.7.4",
    "next-transpile-modules": "^9.0.0",
    "nx": "14.7.13",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16.14"
  },
  "packageManager": "pnpm@7.12.0"
}
